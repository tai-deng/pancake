\n// Learn Attribute:\r\n//  - [Chinese] http://docs.cocos.com/creator/manual/zh/scripting/reference/attributes.html\r\n//  - [English] http://www.cocos2d-x.org/docs/creator/manual/en/scripting/reference/attributes.html\r\n// Learn life-cycle callbacks:\r\n//  - [Chinese] http://docs.cocos.com/creator/manual/zh/scripting/life-cycle-callbacks.html\r\n//  - [English] http://www.cocos2d-x.org/docs/creator/manual/en/scripting/life-cycle-callbacks.html\r\n\r\nconst {ccclass, property} = cc._decorator;\r\n\r\n/**\r\n * 游戏配置\r\n */\r\n@ccclass\r\nexport default class GameConfig {\r\n\r\n    //=============================================================\r\n    //等级配置\r\n    //=============================================================\r\n    static LEVELS:Array<object> = [\r\n        // \"level\":number,                         //等级，比如1, 2, 3...\r\n        // \"score\":[number, number],               //分数区间比如[0, 100]\r\n        // \"panSize\":{number, null},               //0:大, 1:中, 2:小，默认为大\r\n        // \"tl\":{boolean, null},                   //是否限时。默认不限时\r\n        // \"fv\":{number, null},                     //施加力量，可以为空\r\n        // \"g\":{number, null},                      //重力系数，可以为空\r\n        // \"f\":{number, null},                      //摩擦系数，可以为空\r\n        // \"r\":{number, null},                      //弹性系数，可以为空\r\n        {level:1, score:[0, 9],         \"desc\":\"10分升级\",      panSize:null, tl:null, fv:null, g:null, f:null, r:null},\r\n        {level:2, score:[10, 19],       \"desc\":\"20分升级\",      panSize:1, tl:true, fv:null, g:null, f:null, r:null},\r\n        {level:2, score:[20, 29],       \"desc\":\"30分升级\",      panSize:1, tl:0.6, fv:null, g:null, f:null, r:null},\r\n        {level:2, score:[20, 10000],    \"desc\":\"高分永无止境\",   panSize:2, tl:null, fv:null, g:null, f:null, r:null},\r\n    ];\r\n    //=============================================================\r\n\r\n\r\n    //=============================================================\r\n    //游戏主玩法配置\r\n    //=============================================================\r\n    //煎饼分段数量\r\n    static PANCAKE_SLICE_COUNT:number = 12;\r\n    //煎饼重力系数\r\n    static PANCAKE_GRAVITY_SCALE:number = 1;\r\n    //煎饼摩擦系数(0-1)\r\n    static PANCAKE_FRICTION:number = 0.65;\r\n    //煎饼弹性系数(0-1)\r\n    static PANCAKE_RESTITUTION:number = 0.15;\r\n    //煎饼弯曲系数(0-2)\r\n    static PANCAKE_BEND:number = 1.2;\r\n    //煎饼角速度衰减。数值越大，煎饼越难以翻面\r\n    static PANCAKE_ANGULAR_DAMPING:number = 1.1;\r\n\r\n\r\n    //点击屏幕时，施加给手臂的力量（数值越大，抛得越高）\r\n    static PANCAKE_FORCE_VALUE:number = 10000;\r\n    //=============================================================\r\n\r\n\r\n\r\n}\r\n"]}