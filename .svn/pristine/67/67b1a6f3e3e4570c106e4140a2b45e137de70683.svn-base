 // 用户昵称\r\n    userName:cc.Label=null;\r\n\r\n    @property(cc.Node)  // 第一名\r\n    selfOne:cc.Node = null;\r\n    @property(cc.Node)  // 第二名\r\n    selfTwo:cc.Node = null;\r\n    @property(cc.Node)  // 第三名\r\n    selfThree:cc.Node = null;\r\n    @property(cc.Node)  // 其他名次奖牌\r\n    selfFour:cc.Node = null;\r\n    @property(cc.Label) // 其他名次\r\n    selfPlace:cc.Label = null;\r\n\r\n    @property(cc.Node)  // 第一名\r\n    medalOne:cc.Node = null;\r\n    @property(cc.Node)  // 第二名\r\n    medalTwo:cc.Node = null;\r\n    @property(cc.Node)  // 第三名\r\n    medalThree:cc.Node = null;\r\n    @property(cc.Node)  // 其他名次奖牌\r\n    medalFour:cc.Node = null;\r\n    @property(cc.Label) // 其他名次\r\n    place:cc.Label = null;\r\n\r\n    private _data:object;\r\n    get data():object {\r\n        return this._data;\r\n    }\r\n    set data(v:object) {\r\n        if(this._data != v){\r\n            this._data = v;\r\n            this.refreshUI();\r\n        }\r\n    }\r\n    // onLoad () {}\r\n\r\n    start () {\r\n        if(!this._isOnLoadCalled) return;\r\n        this.refreshUI();\r\n    }\r\n\r\n    private refreshUI(){\r\n        let data = this._data;\r\n        for (const key in data) {\r\n            if(data[\"grade\"] == 1){\r\n                this.selfOne.active = true;\r\n                this.medalOne.active = true;\r\n                this.selfTwo.active = false;\r\n                this.medalTwo.active = false;\r\n                this.selfThree.active = false;\r\n                this.medalThree.active = false;\r\n                this.selfFour.active = false;\r\n                this.medalFour.active = false;\r\n            }\r\n            if(data[\"grade\"] == 2){\r\n                this.selfOne.active = false;\r\n                this.medalOne.active = false;\r\n                this.selfTwo.active = true;\r\n                this.medalTwo.active = true;\r\n                this.selfThree.active = false;\r\n                this.medalThree.active = false;\r\n                this.selfFour.active = false;\r\n                this.medalFour.active = false;\r\n            }\r\n            if(data[\"grade\"] == 3){\r\n                this.selfOne.active = false;\r\n                this.medalOne.active = false;\r\n                this.selfTwo.active = false;\r\n                this.medalTwo.active = false;\r\n                this.selfThree.active = true;\r\n                this.medalThree.active = true;\r\n                this.selfFour.active = false;\r\n                this.medalFour.active = false;\r\n            }\r\n            if(data[\"grade\"] == 4){\r\n                this.selfOne.active = false;\r\n                this.medalOne.active = false;\r\n                this.selfTwo.active = false;\r\n                this.medalTwo.active = false;\r\n                this.selfThree.active = false;\r\n                this.medalThree.active = false;\r\n                this.selfFour.active = true;\r\n                this.medalFour.active = true;\r\n                this.selfPlace.getComponent(Label)\r\n            }\r\n        }\r\n    }\r\n    // update (dt) {}\r\n}\r\n"]}